{
  "name": "atom-macaulay2",
  "main": "./lib/atom-macaulay2",
  "version": "1.0.0",
  "description": "A package for executing Macaulay2 commands from Atom. Fork of platformio-atom-ide-terminal.",
  "author": "Jeremy Ebneyamin",
  "keywords": [
    "Macaulay2",
    "algebra",
    "geometry",
    "mathematics"
  ],
  "contributors": [
    {
      "name": "Mahrud Sayrafi",
      "email": "mahrud@umn.edu",
      "url": "http://math.umn.edu/~mahrud/"
    },
    {
      "name": "The Community Contributors",
      "url": "https://github.com/mahrud/atom-macaulay2/graphs/contributors"
    }
  ],
  "repository": "https://github.com/mahrud/atom-macaulay2",
  "license": "MIT",
  "engines": {
    "atom": ">=1.12.2 <2.0.0"
  },
  "dependencies": {
    "atom-space-pen-views": "^2.2.0",
    "node-pty-prebuilt-multiarch": "0.9.0-beta21.legacy",
    "term.js": "https://github.com/jeremyramin/term.js/tarball/master",
    "underscore": "^1.8.3"
  },
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "providedServices": {
    "atomMacaulay2": {
      "description": "Atom Macaulay2",
      "versions": {
        "1.1.0": "provideAtomMacaulay2"
      }
    }
  }
}
